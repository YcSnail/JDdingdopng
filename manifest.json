{
  "name": "jd pang",
  "version": "1.2",
  "manifest_version": 2,
  "description": "自动输入文字",
  "content_scripts": [{
    "matches": ["https://www.baidu.com/*"],
    "js": ["js/jquery-3.2.1.min.js","js/baidu.js"],
    "run_at": "document_end"
  }],

  "permissions": ["storage","notifications"],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
